{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"module": "ESNext",
		"esModuleInterop": false,
		"jsx": "react",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "dist",
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "es5",
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"include": ["./src/**/*"]
}
